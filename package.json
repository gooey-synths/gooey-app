{
  "name": "gooey-app",
  "version": "0.0.0",
  "main": "electron/main.js",
  "author": "Gooey Synths",
  "description": "The gooey app.",
  "scripts": {
    "start": "ng serve",
    "electron": "cross-env NODE_ENV=development electron electron/main.js",
    "build": "ng build",
    "build:electron": "ng build --base-href=./ && cross-env NODE_ENV=production electron-forge make",
    "lint": "ng lint",
    "test": "ng test --watch=false",
    "test:ci": "ng test --no-watch --no-progress --browsers=ChromeHeadless",
    "test:e2e": "playwright test",
    "start:forge": "electron-forge start"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^19.2.0",
    "@angular/compiler": "^19.2.0",
    "@angular/core": "^19.2.0",
    "@angular/forms": "^19.2.0",
    "@angular/platform-browser": "^19.2.0",
    "@angular/platform-browser-dynamic": "^19.2.0",
    "@angular/router": "^19.2.0",
    "@ngrx/store": "^19.0.1",
    "@tailwindcss/postcss": "^4.0.17",
    "electron-log": "^5.3.3",
    "electron-updater": "^6.6.2",
    "ngx-electron": "^2.2.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.4",
    "@angular/cli": "^19.2.4",
    "@angular/compiler-cli": "^19.2.0",
    "@electron-forge/cli": "^7.8.0",
    "@electron-forge/maker-deb": "^7.8.0",
    "@electron-forge/maker-squirrel": "^7.8.0",
    "@electron-forge/maker-zip": "^7.8.0",
    "@electron-forge/publisher-github": "^7.8.0",
    "@playwright/test": "^1.51.1",
    "@types/jasmine": "~5.1.0",
    "angular-eslint": "19.3.0",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "electron": "^35.1.2",
    "eslint": "^9.23.0",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.0.17",
    "typescript": "~5.7.2",
    "typescript-eslint": "8.27.0"
  }
}
